@model Setting
@{
    ViewData["Title"] = "Setting Page";
}
<div class="pt-5">
    <div class="row py-3 justify-content-center">
        <div class="col-10">
            <form asp-action="Create" enctype="multipart/form-data">
                <div class="row">
                    <div class="col-6 mb-3">
                        <label asp-for="Title" class="form-label" ></label>
                        <input asp-for="Title" class="form-control" />
                        <span class="text-danger" asp-validation-for="Title"></span>
                    </div>
                    <div class="col-6 mb-3">
                        <label asp-for="SubTitle" class="form-label" ></label>
                        <textarea asp-for="SubTitle" class="form-control" ></textarea>
                        <span class="text-danger" asp-validation-for="SubTitle"></span>
                    </div>
                </div>
                <div class="row">
                    <div class="col-6 mb-3">
                        <label asp-for="Address" class="form-label" ></label>
                        <textarea asp-for="Address" class="form-control" ></textarea>
                        <span class="text-danger" asp-validation-for="Address"></span>
                    </div>
                    <div class="col-6 mb-3">
                        <label asp-for="ICON" class="form-label" ></label>
                        <textarea asp-for="ICON" class="form-control" ></textarea>
                        <span class="text-danger" asp-validation-for="ICON"></span>
                    </div>
                </div>
                <button type="submit" class="btn btn-success">Create</button>
            </form>
        </div> 
    </div>  
</div>